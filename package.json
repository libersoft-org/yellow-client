{
  "name": "yellow-client",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "init": "bun x --bun svelte-kit sync; bun x --bun @inlang/paraglide-js compile --project ./project.inlang",
    "build": "bun x --bun vite build",
    "preview": "vite preview",
    "test": "npx vitest run; npx playwright test",
    "test:unit": "npx vitest run",
    "test:e2e": "npx playwright test",
    "svelte-check": "svelte-check",
    "typecheck": "svelte-check",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "prettier": "prettier --plugin 'prettier-plugin-svelte' --write \"src/**/*.{js,ts,css,html,svelte,md,json}\"",
    "prepare": "husky",
    "build:messages-service": "npx vite build --config vite.service.config.js && mkdir -p static/tmp && cp dist/services/messages-service.js static/tmp/",
    "watch:messages-service": "node scripts/build-service.js --watch"
  },
  "lint-staged": {
    "*.{js,ts,svelte,html,json,css,md}": "prettier --plugin 'prettier-plugin-svelte' --write"
  },
